    <button type="button" class="btn btn-default" id="stigmod-model-view-fs">
      <span class="fa fa-sitemap"></span>
    </button>
    <button type="button" class="btn btn-default stigmod-model-save-btn" disabled>
      <span class="glyphicon glyphicon-upload"></span>
    </button>
    <button type="button" class="btn btn-default stigmod-exit-full-screen-btn">
      <span class="glyphicon glyphicon-resize-small"></span>
    </button>
  </div>
  <!-- 顶部项目名称栏 -->
  <div class="row stigmod-hide-when-full-screen" id="stigmod-header">
    <div class="col-xs-8">
      <h1 id="stigmod-model-title">
        <span class="fa fa-cube" id="stigmod-model-title-icon"></span>
        <a href="/u"><span id="stigmod-model-title-username"> <%= user.mail %></span></a>
        <span>&nbsp;/&nbsp;</span>
        <span class="dropdown">
          <span class="dropdown-toggle stigmod-a-pseudo" type="button" id="interProjJump" data-toggle="dropdown">
            <%= model %>
          </span>
          <% if (modelInfo.length > 1) { %>
          <ul class="dropdown-menu" role="menu" aria-labelledby="interProjJump">
            <% modelInfo.forEach(function(info) {
                if (model !== info.name) { %>
                  <li role="presentation"><a href="/<%= info.name %>/workspace" role="menuitem" tabindex="-1" href="#"><%= info.name %></a></li>
              <% } %>
            <% }); %>
          </ul>
          <% } %>
        </span>&nbsp;
        <span class="stigmod-a-pseudo stigmod-a-nounderline stigmod-model-save"><span class="glyphicon glyphicon-upload"></span></span>
        <span class="stigmod-model-saved"><span class="glyphicon glyphicon-upload"></span></span>
      </h1>
    </div>
    <div class="col-xs-4">
      <span class="pull-right stigmod-btn-left" id="stigmod-model-info">
        <a href="/<%= model %>/info" class="stigmod-a-nounderline">
          <span class="glyphicon glyphicon-stats"></span>&nbsp;
          Model Info
        </a>
      </span>
      <span class="pull-right stigmod-btn-left">&nbsp;&nbsp;|&nbsp;&nbsp;</span>
      <span class="pull-right stigmod-btn-left" id="stigmod-model-view">
        <span class="stigmod-a-pseudo stigmod-a-nounderline">
          <span class="fa fa-sitemap"></span>&nbsp;
          Model View
        </span>
      </span>
      <span class="pull-right stigmod-btn-left">&nbsp;&nbsp;|&nbsp;&nbsp;</span>
      <span class="pull-right stigmod-btn-left stigmod-enter-full-screen-btn">
        <span class="stigmod-a-pseudo stigmod-a-nounderline">
          <span class="glyphicon glyphicon-fullscreen"></span>&nbsp;
          Full Screen
        </span>
      </span>
    </div>
  </div>
  <hr class="stigmod-hide-when-full-screen" />
