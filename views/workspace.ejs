
<div id="stigmod-pg-workspace">
  <!-- 遮罩 -->
  <div class="stigmod-mask-white"></div>
  <!-- 等待动画 -->
  <div class="stigmod-loader stigmod-align-center-1">
    <span></span>
    <span></span>
    <span></span>
  </div>
  <!-- 等待文字 -->
  <div class="stigmod-loader-text stigmod-align-center-1"><p> sending...</p></div>
