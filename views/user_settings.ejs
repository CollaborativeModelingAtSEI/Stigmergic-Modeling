  var dataPassedIn = <%-JSON.stringify(data) %>;  // 获取后端模板传来的参数 data

  seajs.config({
    paths: {
      'js': 'http://localhost:3000/src/js'
    }
  });
  seajs.use('js/app/user_settings.js');
</script>